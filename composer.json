{
    "name": "rxante/oauth",
    "description": "A collection of OAuth related things for the RxAnte Web Services team",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "RxAnte Web Services"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-curl": "*",
        "ext-mbstring": "*",
        "dflydev/fig-cookies": "^3.1",
        "firebase/php-jwt": "^6.10",
        "guzzlehttp/guzzle": "^7.0",
        "hyperf/guzzle": "^3.1",
        "lcobucci/jwt": "^5.5",
        "league/oauth2-client": "^2.7",
        "league/oauth2-server": "^9.2",
        "psr/cache": "^3.0",
        "psr/clock": "^1.0",
        "psr/container": "^2.0",
        "psr/http-factory": "^1.1",
        "psr/http-message": "^2.0",
        "psr/http-server-middleware": "^1.0",
        "psr/log": "^2 || ^3",
        "ramsey/uuid": "^4.7"
    },
    "require-dev": {
        "rxante/coding-standards": "^1.1.2"
    },
    "autoload": {
        "psr-4": {
            "RxAnte\\OAuth\\": "php/src"
        },
        "files": ["php/Polyfill/JsonValidate.php"]
    },
    "config": {
        "bin-compat": "full",
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
