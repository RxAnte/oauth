{
    "name": "rxante/oauth",
    "description": "A collection of OAuth related things for the RxAnte Web Services team",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "RxAnte Web Services"
        }
    ],
    "require": {
        "php": "^8.2",
        "psr/container": "^2.0",
        "psr/http-server-middleware": "^1.0",
        "psr/log": "^2 || ^3"
    },
    "autoload": {
        "psr-4": {
            "RxAnte\\OAuth\\": "php/src"
        }
    },
    "config": {
        "bin-compat": "full",
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "require-dev": {
        "doctrine/coding-standard": "^12.0",
        "phpcsstandards/php_codesniffer": "^3.10",
        "phpstan/phpstan": "^1.12",
        "phpstan/phpstan-deprecation-rules": "^1.2",
        "phpstan/phpstan-strict-rules": "^1.6"
    }
}
